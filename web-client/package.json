{
  "name": "mrdario-client-web",
  "description": "Mr. Dario - a Dr. Mario clone in ES6",
  "author": "Dan Delany <dan.delany@gmail.com>",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "github:dandelany/mrdario",
  "scripts": {
    "build": "yarn run clean-build && webpack -p --config=configs/webpack/prod.js",
    "clean-build": "rm -f -r -d build",
    "format": "prettier --write \"src/**/*.{ts,tsx}\" \"src/**/*.{js,jsx}\"",
    "lint": "tslint './src/**/*.ts*' --format stylish --force --fix",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/common.js",
    "start-prod": "yarn run build && cd build && python -m SimpleHTTPServer 6969",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "release": "yarn run lint && yarn run format && yarn run build"
  },
  "dependencies": {
    "@ircam/sync": "^2.0.0",
    "classnames": "^2.2.6",
    "d3-random": "^1.1.2",
    "driftless": "^2.0.3",
    "es6-promisify": "^6.0.1",
    "lodash": "^4.17.11",
    "mrdario-core": "2.0.0",
    "pixi-filters": "^2.7.0",
    "pixi-particles": "^3.1.0",
    "pixi.js": "^4.8.5",
    "rc-slider": "^8.6.4",
    "react": "^16.7.0",
    "react-container-dimensions": "^1.4.1",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "react-spring": "^8.0.7",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "socketcluster-client": "^14.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.0",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.3.1",
    "@types/d3-random": "^1.1.1",
    "@types/es6-promisify": "^6.0.0",
    "@types/jest": "^24.0.5",
    "@types/lodash": "^4.14.120",
    "@types/node": "^11.9.4",
    "@types/pixi.js": "^4.8.6",
    "@types/rc-slider": "^8.6.0",
    "@types/react": "^16.7.20",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "@types/socketcluster-client": "^13.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^27.0.5",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "node-sass": "^4.14.1",
    "raw-loader": "^1.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.13.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "1.1.3",
    "style-loader": "^0.23.1",
    "svgo-loader": "^2.2.0",
    "tslint": "^5.12.1",
    "typescript": "3.9.8",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.2.1",
